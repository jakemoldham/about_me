<%= form_for @contact, url: contact_path do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %> 
  <%= f.text_field :email, :maxlength => 32, class: '' %>

  <%= f.label :message %>
  <%= f.text_area :message, class: ''  %>
  <%= f.submit "Submit", class: '' %>

<% end %>
